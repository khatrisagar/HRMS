<div class="attendance-summary">
    <div class="row">
        <div class="col_7">
            <div class="box">
                <label> Days</label>
                <p>15</p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Late</label>
                <p>0% (0 Days)</p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Absent</label>
                <p>0% (0 Days) </p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Half Days</label>
                <p>0</p>
            </div>
        </div>



        <div class="col_7">
            <div class="box">
                <label> Total Office</label>
                <p>153 hrs </p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Total worked</label>
                <p>122 hrs 10 mins</p>
            </div>
        </div>

        <div class="col_7">
            <div class="box">
                <label> Productivity Ratio</label>
                <p>90.49%</p>
            </div>
        </div>

    </div>


    <div class="table">
        <table>
            <tr style="background-color: #002f4b;">
                <th>Date </th>
                <th>Entry Time </th>
                <th>Exit time </th>
                <th>Break time </th>
                <th>Working Hours </th>

            </tr>

            <% for(i=0;i<data.length;i++) {%>
                <tr>
                    <td>
                        <%=data[i].check_date %>
                    </td>
                    <td>
                        <%=data[i].checkin_time %>
                    </td>
                    <td>
                        <%=data[i].checkout_time %>
                    </td>

                    <td><span><%=data[i].total_office_time %> </span><progress id="file" value="32" max="100"></progress></td>

                    <td>

                    </td>
                </tr>
                <%} %>

        </table>
    </div>
</div>